<h1>Add a Movie to <%= @list.name %></h1>

<div class="form-align">
  <div class="form">
    <%= simple_form_for [@list, @bookmark] do |f| %>
      <%= f.input :comment %>
      <%= f.association :movie %>
      <%= f.submit "Create bookmark", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
